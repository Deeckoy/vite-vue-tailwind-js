{
	"name": "vite-project",
	"version": "0.0.0",
	"type": "commonjs",
	"scripts": {
		"dev": "cross-env NODE_ENV=development vite --port 4000",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"lint": "eslint --ext .vue --ext .ts --ext .js ./src/",
		"lint:fix": "eslint --ext .vue --ext .ts --ext .js ./src/ --fix",
		"format": "prettier --write ./src/**/*.{js,ts,vue,scss,css,html,json} && prettier --write ./index.html",
		"analysis": "windicss-analysis"
	},
	"dependencies": {
		"@headlessui/vue": "^1.7.0",
		"@heroicons/vue": "^1.0.6",
		"pinia": "^2.0.16",
		"vue": "^3.2.37",
		"vue-i18n": "^9.1.10",
		"vue-router": "^4.1.2"
	},
	"devDependencies": {
		"@intlify/vite-plugin-vue-i18n": "^5.0.1",
		"@types/node": "^18.0.6",
		"@vitejs/plugin-vue": "^2.3.3",
		"autoprefixer": "^10.4.8",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.13",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^8.7.1",
		"postcss": "^8.4.16",
		"prettier": "2.5.1",
		"sass": "^1.53.0",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.7.4",
		"vite": "^2.9.14",
		"vue-demi": "^0.13.11",
		"vue-tsc": "^0.29.8"
	}
}
